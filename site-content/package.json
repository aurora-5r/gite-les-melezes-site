{
  "name": "aurora-5r",
  "version": "1.0.0",
  "description": "Aurora-5r.fr",
  "repository": "laurentmau/aurora-5r-site",
  "main": "index.bundle.js",
  "scripts": {
    "eleventy:default": "npx eleventy",
    "eleventy:optim": "node src/scripts/generate-responsive-imgs && eleventy",
    "build": "$npm_execpath run build:webpack && $npm_execpath run build:eleventy",
    "build:eleventy": "ELEVENTY_ENV=production $npm_execpath run eleventy:optim",
    "build:eleventy:preview": "npx eleventy --serve",
    "build:preview": "$npm_execpath run build:webpack &&  $npm_execpath run build:eleventy:preview",
    "build:webpack": "NODE_ENV=production webpack --config webpack.prod.js",
    "lint": "run-p lint:**",
    "lint:css": "stylelint \"site/assets/scss/**/*.scss\"",
    "lint:js": "eslint .",
    "preview": "run-p preview:**",
    "preview:eleventy": "npx eleventy --serve",
    "preview:webpack": "$npm_execpath run start:webpack",
    "start": "$npm_execpath run start:webpack &&  $npm_execpath run start:eleventy",
    "start:eleventy": "npx eleventy --serve",
    "start:webpack": "webpack --watch   --config webpack.dev.js --hot"
  },
